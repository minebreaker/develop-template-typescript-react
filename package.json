{
  "name": "develop-template-typescript-react",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "npm run assemble && npm run test",
    "build:production": "export NODE_ENV=production && npm run clean && npm run assemble",
    "assemble": "npm run compile && npm run bundle && npm run copy",
    "compile": "tsc -p tsconfig.json",
    "bundle": "webpack --display-error-details",
    "copy": "cpx static/index.html ./build/out",
    "test": "mocha --config .mocharc.js",
    "clean": "rimraf ./build/*",
    "serve": "cd build/out && python -m http.server",
    "watchCompile": "tsc -p tsconfig.json --incremental -w",
    "watchBundle": "webpack -w"
  },
  "dependencies": {
    "@date-io/luxon": "^2.4.0",
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "core-js": "^3.6.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "luxon": "^1.22.0",
    "ramda": "^0.27.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router5": "^7.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-router5": "^7.0.2",
    "redux-thunk": "^2.3.0",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@types/chai": "^4.2.9",
    "@types/enzyme": "^3.10.5",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/mocha": "^7.0.1",
    "@types/ramda": "^0.26.41",
    "@types/react": "^16.9.21",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.16",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }

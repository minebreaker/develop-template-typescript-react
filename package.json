{
  "name": "develop-template-typescript-react",
  "version": "0.1.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "npm run assemble && npm run test",
    "build:production": "export NODE_ENV=production && npm run clean && npm run assemble",
    "assemble": "npm run compile && npm run bundle && npm run copy",
    "compile": "tsc -p tsconfig.json",
    "bundle": "webpack --display-error-details",
    "copy": "cpx static/index.html ./build/out",
    "test": "mocha --config .mocharc.js",
    "clean": "rimraf ./build/*",
    "serve": "cd build/out && python -m http.server",
    "watchCompile": "tsc -p tsconfig.json --incremental -w",
    "watchBundle": "webpack -w"
  },
  "dependencies": {
    "@date-io/luxon": "^1.3.8",
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/pickers": "^3.2.0",
    "@material-ui/styles": "^4.2.1",
    "core-js": "^3.1.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.14",
    "luxon": "^1.17.1",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router5": "^7.1.0",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-router5": "^7.0.2",
    "redux-thunk": "^2.3.0",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.10.3",
    "@types/lodash": "^4.14.136",
    "@types/luxon": "^1.15.2",
    "@types/mocha": "^5.2.7",
    "@types/ramda": "^0.26.16",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "^16.8.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cpx": "^1.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.12",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  }
